
<h2 class="page-header"></h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<form method="post" action="/newacc/register2">
   <div class="form-group">
    <label>Interests:</label>
    <input type="text" value="" data-role="tagsinput" id="tags" class="form-control">
    {{#if interests}}
      {{#each interests}}
        <input type="number" class = "form-control">
      {{/each}}
    {{/if}}
   <div class="form-group">
    <label>Skills:</label>
    <input type="text" class="form-control" placeholder="Name" name="name">
   </div>
   <div class="form-group">
    <label>Github:</label>
    <input type="text" class="form-control" placeholder="Name" name="github">
   </div>
   <div class="form-group">
    <label>Devpost:</label>
    <input type="text" class="form-control" placeholder="Name" name="devpost">
   </div>
   <div class="form-group">
    <label>Linkedin:</label>
    <input type="text" class="form-control" placeholder="Name" name="linkedin">
   </div>
   <div class="form-group">
    <label>Personal Website:</label>
    <input type="text" class="form-control" placeholder="Name" name="website">
   </div>
   <div class="form-group">
    <label>How many hackathons have you been to?</label>
    <input type="number" class="form-control" placeholder="Hackthons" name = "numOfHacks">
    <!--
    <select name="hacknum">
        <option value="0">Going to my first one!</option>
        <option value="1-3">1-3</option>
        <option value="3-6">3-6</option>
        <option value="7-10">7-10</option>
        <option value="10-15">10-15</option>
        <option value="15+">15+</option>
    </select>
  -->
    <button type ="submit" class="btn btn-default">Submit</button>
   </div>

</form>
